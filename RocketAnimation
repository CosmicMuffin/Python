import os
import time

for i in range(0,50):
    print(i*' ','\\\\\\')
    print(i*' ','  \\\\\\|||||||||||||||||||||||||\\\\')
    print(i*' ','    \\\\\\         space          \\\\')
    print(i*' ','    ///    rocket i guess      //')
    print(i*' ','  ///|||||||||||||||||||||||||//')
    print(i*' ','///')
    time.sleep(0.09)
    os.system('cls')
